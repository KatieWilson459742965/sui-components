{
  "name": "@schibstedspain/sui-card-product",
  "version": "1.2.0",
  "description": "SUI Card Product",
  "main": "lib/index.js",
  "scripts": {
    "build": "rm -Rf ./lib && mkdir -p ./lib && npm run babel && npm run sass",
    "babel": "../../../node_modules/.bin/babel ./src --out-dir ./lib",
    "sass": "../../../node_modules/.bin/cpx \"./src/**/*.scss\" ./lib",
    "preversion": "echo \"preversion\"",
    "version": "npm run build",
    "postversion": "git add ${PWD} && git commit -m \"release(card/product): v$(node -p -e \"require('./package.json').version\")\" && git push origin master && npm publish --access=public"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@schibstedspain/sui-svgiconset": "1",
    "@schibstedspain/sui-tag-chip": "1",
    "@schibstedspain/suistudio-fatigue-deps": "github:SUI-Components/suistudio-fatigue-deps",
    "swiper": "3.4.1"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-3"
    ],
    "plugins": [
      "transform-runtime",
      "transform-class-properties"
    ]
  }
}
